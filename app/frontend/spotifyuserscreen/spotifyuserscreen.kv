<SpotifyUserScreen>:
    name: "spotifyuserscreen"

    MDFloatLayout:

        MDBottomNavigation:
            panel_color: "#E0E0E0"
            text_color_active: "#080808"
            use_text: False

            MDBottomNavigationItem:
                name: "spotifymainscreen"
                md_bg_color: "#080808"
                text: "main"
                icon: "home"

            MDBottomNavigationItem:
                name: "spotifyhistoryscreen"
                md_bg_color: "#080808"
                text: "History"
                icon: "history"

            MDBottomNavigationItem:
                name: "spotifytoplistscreen"
                md_bg_color: "#080808"
                text: "Top List"
                icon: "arrow-top-right"
